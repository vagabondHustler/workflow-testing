{
	"categories": [
		{
			"title": "**✨ Features:**",
			"labels": ["feature"]
		},
		{
			"title": "**🐛 Fixes:**",
			"labels": ["fix"]
		},
		{
			"title": "**🧪Tests:**",
			"labels": ["test"]
		},
		{
			"title": "**🚧 Chores:**",
			"labels": ["chore", "other"]
		}
	],
	"pr_template": "${{TEST}} - ${{TITLE}}",
	"custom_placeholders": [
    {
      "name": "TEST",
      "source": "BODY",
      "transformer": {
        "pattern": ".*",
        "target": "```"
      }
    }
  ]
}
